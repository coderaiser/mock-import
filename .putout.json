{
    "rules": {
        "putout": "on"
    },
    "match": {
        "example": {
            "remove-console": "off"
        },
        "README.md": {
            "esm/merge-duplicate-imports": "off"
        },
        "test/errors.ts": {
            "tape/add-await-to-re-import": "off"
        },
        "lib/mock-import.spec.js": {
            "tape/remove-stop-all": "off"
        },
        "*.md": {
            "esm/declare-imports-first": "off"
        }
    }
}
